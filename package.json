{
  "name": "@djforth/cookie_mgmt_fp",
  "version": "1.0.2",
  "description": "Cookies manager",
  "main": "index.js",
  "scripts": {
    "build": "babel src -x '.js'  -d lib",
    "build:watch": "babel src -x '.js' -w --source-maps  -d lib",
    "lint": "eslint src/**",
    "lint:tests": "eslint spec/**",
    "test": "npm run lint && ./node_modules/.bin/karma start --browsers PhantomJS --single-run",
    "test:watch": "karma start --browsers PhantomJS",
    "test:mac": "karma start --single-run --browsers Chrome,Firefox,Safari"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015"
          ]
        }
      ]
    ]
  },
  "js_config": {
    "ext": [
      ".js",
      ".es6.js"
    ],
    "files": [
      "cookie_mgmt.js"
    ],
    "input": "src",
    "output": "dist",
    "common": "common.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/djforth/cookie_mgmt_fp.git"
  },
  "author": "A Stainforth",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/djforth/cookie_mgmt_fp/issues"
  },
  "homepage": "https://github.com/djforth/cookie_mgmt_fp#readme",
  "devDependencies": {
    "babel": "^6.5.1",
    "babel-cli": "^6.5.1",
    "babel-eslint": "^5.0.0-beta6",
    "babel-polyfill": "^6.5.0",
    "babel-preset-es2015": "^6.5.0",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "eslint": "^1.10.3",
    "eslint-config-google": "^0.3.0",
    "eslint-plugin-react": "^3.16.1",
    "jasmine-core": "^2.4.1",
    "karma": "^0.13.19",
    "karma-babel-preprocessor": "^6.0.1",
    "karma-browserify": "^5.0.1",
    "karma-chrome-launcher": "^0.2.2",
    "karma-firefox-launcher": "^0.1.7",
    "karma-jasmine": "^0.3.7",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-safari-launcher": "^0.1.1",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-story-reporter": "^0.3.1",
    "lodash": "^4.6.0",
    "phantomjs-prebuilt": "^2.1.3",
    "rewireify": "^0.2.5",
    "through": "^2.3.8",
    "uglify-js": "^2.6.1",
    "watchify": "^3.7.0"
  }
}
