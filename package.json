{
  "name": "@djforth/cookie_mgmt_fp",
  "version": "1.0.0",
  "description": "Cookies manager",
  "main": "index.js",
  "scripts": {
    "build": "babel src -x '.js' --source-maps  -d lib",
    "build:watch": "babel src -x '.js' -w --source-maps  -d lib",
    "javascript:build": "node ./fe_build_scripts/browserify_build.js",
    "javascript:watch": "node ./fe_build_scripts/browserify_build.js -w",
    "lint": "eslint src/**",
    "lint:tests": "eslint spec/**",
    "test": "npm run lint && ./node_modules/.bin/karma start fe_build_scripts/karma.conf.js --browsers PhantomJS --single-run",
    "test:watch": "karma start fe_build_scripts/karma.conf.js --browsers PhantomJS",
    "test:mac": "karma start fe_build_scripts/karma.conf.js --single-run --browsers Chrome,Firefox,Safari"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015",
            "react"
          ]
        }
      ]
    ]
  },
  "js_config": {
    "ext": [
      ".js",
      ".es6.js"
    ],
    "files": [
      "cookie_mgmt.js"
    ],
    "input": "src",
    "output": "dist",
    "common": "common.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/djforth/cookie_mgmt_fp.git"
  },
  "author": "A Stainforth",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/djforth/cookie_mgmt_fp/issues"
  },
  "homepage": "https://github.com/djforth/cookie_mgmt_fp#readme",
  "dependencies": {
    "lodash": "^4.0.0"
  },
  "devDependencies": {
    "babel": "^6.3.26",
    "babel-cli": "^6.4.0",
    "babel-eslint": "^5.0.0-beta6",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "eslint": "^1.10.3",
    "jasmine-core": "^2.4.1",
    "karma": "^0.13.19",
    "karma-browserify": "^4.4.2",
    "karma-chrome-launcher": "^0.2.2",
    "karma-coverage": "^0.5.3",
    "karma-firefox-launcher": "^0.1.7",
    "karma-jasmine": "^0.3.6",
    "karma-phantomjs-launcher": "^0.2.3",
    "karma-safari-launcher": "^0.1.1",
    "karma-story-reporter": "^0.3.1",
    "phantomjs": "^1.9.19",
    "rewireify": "^0.2.5",
    "uglify-js": "^2.6.1",
    "watchify": "^3.7.0"
  }
}
